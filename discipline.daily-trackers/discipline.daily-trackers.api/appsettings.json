{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppOptions": {
    "Name": "discipline.daily-trackers"
  },
  "MongoDbOptions": {
    "ConnectionString":"mongodb://localhost:6011"
  },
  "RabbitMqOptions": {
    "HostName": "localhost",
    "Port": 6014,
    "Username": "discipline",
    "Password": "discipline",
    "VirtualHost": "discipline",
    "Routes": {
      "CreateEmptyUserDailyTrackerCommand": {
        "Exchange": "discipline.activity-scheduler",
        "RoutingKeys": ["create-empty-user-daily-tracker-command"]
      },
      "CreateActivitiesFromActivityRulesCommand": {
        "Exchange": "discipline.centre.activityrules",
        "RoutingKeys": ["create-activity-command"]
      }
    }
  }
}
